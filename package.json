{
  "name": "pinyinsort",
  "version": "1.1.2",
  "main": "dist/index.js",
  "license": "MIT",
  "files": [
    "./"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "release": "standard-version && git push --follow-tags origin main && yarn publish",
    "release:100": "yarn release -- major",
    "release:010": "yarn release -- minor",
    "release:001": "yarn release -- patch",
    "typecheck": "tsc --noEmit",
    "compile": "tsc --project tsconfig.json",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/config-conventional": "^9.1.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.2.13",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.5.3",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "emoji-regex": "^9.2.2",
    "lodash": "^4.17.21",
    "tiny-pinyin": "^1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meijintao233/pinyinsort.git"
  },
  "bugs": {
    "url": "https://github.com/meijintao233/pinyinsort/issues"
  },
  "homepage": "https://github.com/meijintao233/pinyinsort#readme",
  "keywords": [
    "pinyin",
    "sort"
  ]
}
